<div id="performanceTimeGraph" style="width: 100%; height: 400px;">
</div>
<script type="text/javascript">
$(function () {
    var d1 = {
        label: "{{ route.getMeasuredBy }}", 
        data: {{ route.getMeasurePlots(true)|json_encode|raw }},
        hoverable: true,
        clickable: true,
    };
    $.plot($("#performanceTimeGraph"), [ d1 ], {
        xaxis: {
            mode: "time",
        },
        legend: {
            show: false,
        }
    });
});
</script>

